  "devDependencies": {
    "chai": "4.1.1",
    "chai-as-promised": "7.1.1",
    "eslint": "4.7.1",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "3.5.0",
    "nock": "9.0.16",
    "rewire": "2.5.2",
    "sinon": "3.3.0",
    "sinon-chai": "2.13.0",
    "supertest": "3.0.0",
    "tmp": "0.0.33",
